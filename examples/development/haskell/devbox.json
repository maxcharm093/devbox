{
  "packages": [
    "ghc@latest",
    "gmp@latest",
    "stack@latest",
    "cabal-install@latest",
    "zlib@latest",
    "hpack@latest"
  ],
  "shell": {
    "init_hook": null,
    "scripts": {
      "build": [
        "cd my-project",
        "stack build"
      ],
      "start": [
        "cd my-project",
        "stack exec my-project-exe"
      ],
      "run_test": [
        "devbox run build",
        "devbox run start"
      ]
    }
  }
}
